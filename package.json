{
  "name": "pixel-cms",
  "version": "1.0.0",
  "description": "A blank template to get started with Payload 3.0",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "payload generate:importmap && cross-env NODE_OPTIONS=--no-deprecation ESLINT_NO_DEV_ERRORS=true next build",
    "build:clean": "rm -rf .next && cross-env NODE_OPTIONS=--no-deprecation ESLINT_NO_DEV_ERRORS=true next build",
    "dev": "cross-env NODE_OPTIONS=--no-deprecation next dev",
    "devsafe": "rm -rf .next && cross-env NODE_OPTIONS=--no-deprecation next dev",
    "generate:importmap": "cross-env NODE_OPTIONS=--no-deprecation payload generate:importmap",
    "generate:types": "cross-env NODE_OPTIONS=--no-deprecation payload generate:types",
    "lint": "cross-env NODE_OPTIONS=--no-deprecation next lint",
    "payload": "cross-env NODE_OPTIONS=--no-deprecation payload",
    "start": "cross-env NODE_OPTIONS=--no-deprecation next start"
  },
  "dependencies": {
    "@payloadcms/db-postgres": "^3.76.1",
    "@payloadcms/next": "^3.76.1",
    "@payloadcms/payload-cloud": "latest",
    "@payloadcms/richtext-lexical": "^3.76.1",
    "@tailwindcss/forms": "^0.5.7",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/node": "^22.5.4",
    "@types/react": "19.0.7",
    "@types/react-dom": "19.0.3",
    "autoprefixer": "^10.4.24",
    "cross-env": "^10.1.0",
    "date-fns": "^4.1.0",
    "graphql": "^16.12.0",
    "jsonwebtoken": "^9.0.3",
    "next": "^16.1.6",
    "payload": "^3.76.1",
    "postcss": "^8.5.6",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "sharp": "^0.34.5",
    "tailwindcss": "^3.4.19",
    "typescript": "^5.9.3"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "eslint": "^9.16.0",
    "eslint-config-next": "15.1.5",
    "prettier": "^3.4.2"
  },
  "engines": {
    "node": "^18.20.2 || >=20.9.0",
    "pnpm": "^9"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "sharp"
    ]
  }
}
